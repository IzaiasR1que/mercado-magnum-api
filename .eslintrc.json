{
    "env": {
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "no-undef": "off",
        "class-methods-use-this": "off",
        "camelcase": "off"
    }
}
